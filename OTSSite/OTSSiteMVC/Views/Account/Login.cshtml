@model OTSSiteMVC.Models.LoginDto
@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<form class="form-control" id="loginForm">
    <div class="form-group">
        <label asp-for="UserName"></label>
        <input asp-for="UserName" type="text" id="userName"/>
    </div>
    <div class="form-group" id="password">
        <label asp-for="Password"></label>
        <input asp-for="Password" type="password" id="password"/>
    </div>
    <div class="form-group">
        <input type="submit" id="submitButton"/>
    </div>
</form>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/scripts/AccountScripts.js"></script>